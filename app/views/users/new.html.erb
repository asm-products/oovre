<%= form_for @user do |f| %>

	<%= f.label(:username) %>:
	<%= f.text_field(:username) %>
	<br>
	<%= f.label(:first_name) %>:
	<%= f.text_field(:first_name) %>
	<br>
	<%= f.label(:last_name) %>:
	<%= f.text_field(:last_name) %>
	<br>
	<%= f.label(:email) %>:
	<%= f.email_field(:email) %>
	<br>
	<%= f.label("Password") %>:
	<%= f.password_field(:password_hash) %>
	<br>

	<%= f.submit "Create account" %>
<% end %>